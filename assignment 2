//Program to check the number is armstrong or not//
#include<stdio.h>
#include<conio.h>
void main()
{
	int n,r,temp,s=0;
	printf("enter the number");
	scanf("%d",&n);
	temp=n;
	while(n!=0)
	{
		r=n%10;
		s=s+r*r*r*r;
		n=n/10;
	
	}
	if(temp==s)
	printf("armstrong");
	else
	printf("Not armstrong");
	
}
